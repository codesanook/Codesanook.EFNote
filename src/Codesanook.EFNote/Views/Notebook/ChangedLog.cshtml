@model IEnumerable<NotebookChangedLogViewModel>
@{
  ViewData["Title"] = "Notebook - changed log";
}

<div class="main-panel">
  <div class="col-12">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Id</th>
          <th>Name</th>
          <th>Valid from</th>
          <th>Valid to</th>
        </tr>
      </thead>
      <tbody>
        @foreach (var item in Model)
        {
          <tr>
            <td>@item.Notebook.Id</td>
            <td>@item.Notebook.Name</td>
            <td>@item.ValidFrom</td>
            <td>@item.ValidTo</td>
          </tr>
        }
      </tbody>
    </table>

  </div>
</div>
